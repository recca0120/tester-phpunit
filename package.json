{
  "name": "tester-mocha",
  "main": "./lib/tester-mocha",
  "author": "yacut",
  "version": "0.2.1",
  "description": "Mocha test runner provider for the interactive tester",
  "keywords": [
    "test",
    "tester",
    "testing",
    "interactive",
    "mocha",
    "wallaby.js"
  ],
  "repository": "https://github.com/yacut/tester-mocha",
  "bugs": {
    "url": "https://github.com/yacut/tester-mocha/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "(apm test) && (flow check) && (eslint . )"
  },
  "configSchema": {
    "scopes": {
      "title": "List of scopes to run Mocha on.",
      "type": "array",
      "default": [
        "test/*.js"
      ],
      "items": {
        "type": "string"
      }
    },
    "binaryPath": {
      "title": "Path to the global mocha binary",
      "description": "Optionally specify the path to the global mocha binary",
      "type": "string",
      "default": ""
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.4.1",
    "error-stack-parser": "^2.0.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0"
  },
  "package-deps": [
    "tester"
  ],
  "providedServices": {
    "tester": {
      "versions": {
        "1.0.0": "provideTester"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "jasmine-fix": "^1.0.1"
  }
}
